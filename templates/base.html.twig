<body>
<header class="main-header">
    <div class="top-header">
        <div class="container">
            <a href="" class="mobile-catalog-toggle js-mobile-catalog-toggle">
                <i class="fa fa-bars"></i>
                Catalogue
            </a>
            <ul class="top-menu clear-list">
                <li>
                    <a href="" title="Blog">Blog</a>
                </li>
                <li>
                    <a href="" title="Contacts">Contacts</a>
                </li>
            </ul>
            <ul class="social clear-list">
                <li>
                    <a href="https://www.instagram.com/saveursdarmenie/">
                        <i class="fa fa-instagram"></i>
                    </a>
                </li>
                <li>
                    <a href="https://www.facebook.com/nazelie.hunanyan">
                        <i class="fa-brands fa-facebook">
                        </i>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="header-content">
        <div class="container">
            <a href="" class="logo" title="Saveurs d'Armenie">
                {#                <i class="icon icon-logo"></i> #}
                Saveurs d'Armenie
            </a>
            <div class="phone">
                <i class="icon-phone icon"></i>
                <a href="tel:0617336170" class="tel">0617336170</a>
            </div>
        </div>
    </div>
    <div class="catalog d-none d-lg-block">
        <div class="container">
            {#            <div class="row flex-center"> #}
            <ul class="main-menu">
                {% for category in categories %}
                    <li class="separator"></li>
                    <li class="level-1">
                        <a href="">
                            {{ category.name }}
                        </a>
                        <ul class="level-2">
                            {% for subcat in category.subcats %}
                                <li class="" data-handle="">
                                    <a title="{{ subcat.name }}" href="" class="menu-link">
                                        <span class="link-text">{{ subcat.name }}</span>
                                    </a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                {% endfor %}
            </ul>
            {#            </div> #}
        </div>
    </div>
</header>

{#    {{ dump(app.user) }} #}

{% block body %}{% endblock %}

<footer>
    <div class="container">
        <div class="d-flex flex-wrap">
            <div class="col-lg-3 col-md-5 col-sm-4 col-xs-12 foot-logo">
                <a href="" title="">
                    <i class="icon icon-logo"></i>
                </a>
                <div class="copy">
                    <p><span>SAS "Saveurs d'Armenie", 2017<br> Tous les droits sont réservés</span></p>
                </div>
            </div>
            <div class="col-lg-4 col-sm-8 col-md-7 col-xs-12 foot-menus">
                <div class="title">
                    Catalogue
                </div>
                <ul class="clear-list">
                    {% for category in categories %}
                        <li>
                            <a href="" title="{{ category.name }}" class="menu-link">
                                <span class="link-text">{{ category.name }}</span>
                            </a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
            {#            <div class="d-lg-block col-sm-1 col-md-1 col-xs-1"></div> #}
            <div class="col-lg-5 col-md-12 col-xs-12 foot-contacts">
                <div class="title">
                    Nos contacts
                </div>
                <div class="foot-contacts-inner">
                    <div class="phone">
                        <i class="icon-phone icon"></i>
                        <a href="tel:0617336170" class="tel">0617336170</a>
                    </div>
                    <div class="address">
                        <i class="icon icon-place"></i>
                        <div class="address-block">
                            <p>80000, France, <br> Amiens, 219 Av. Louis Blanc</p>
                        </div>
                    </div>
                </div>
                <div class="foot-social">
                    <div class="social-links"><span class="hide-xs">Dans les réseaux sociaux:</span>
                        <ul class="social clear-list">
                            <li>
                                <a href="https://www.instagram.com/saveursdarmenie/">
                                    <i class="fa fa-instagram"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.facebook.com/nazelie.hunanyan">
                                    <i class="fa-brands fa-facebook">
                                    </i>
                                </a>
                            </li>
                        </ul>
                    </div>
                    {#                    <div class="insales"> #}
                    {#                        powered by <a href=""></a> #}
                    {#                    </div> #}
                </div>
            </div>
        </div>
    </div>
</footer>

